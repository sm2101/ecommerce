 })\n        setSubCatIds([])\n        }\n    \n\n    useEffect(() =>{\n        loadProduct();\n        loadCategories();\n\n    },[])\n\n    const handleSubmit = (e) =>{\n        e.preventDefault();\n        setLoading(true)\n        values.subCat = subCatIds;\n        updateProduct(slug,values,user.token).then(res =>{\n            console.log(res)\n            toast.success(\"Product updated!\");\n            history.push(\"/admin/products\")\n            setLoading(false);\n        }).catch(err =>{\n            console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Update Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n             <UpdateProdductForm \n             getSubs = {getSubs} \n             handleChange = {handleChange} \n             handleSubmit = {handleSubmit} \n             values = {values} \n             categories = {categories}\n             subOptions = {subOptions}\n             setValues = {setValues}\n             subCatIds = {subCatIds}\n             setSubCatIds = {setSubCatIds}\n             />\n          </div>\n          </div>\n        </div>\n      </>\n    )\n}\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/SubCategory/CreateSubCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/AdminRoutes.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/UserRoute.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/category.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Nav/AdminNav.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/subCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/products.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/LoadingToRedirect.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CategoryForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CreateProductForm.js",["265","266","267"],"import React from 'react';\nimport {Select} from 'antd';\nconst {Option} = Select;\nconst CreateProductForm = (\n    {\n        values,\n        handleSubmit,\n        handleChange,\n        getSubs,\n        subOptions,\n        subDropDown,\n        setValues\n    }\n) => {\n    const {title,\n        description,\n        price,\n        category,\n        categories,\n        subCat,\n        shipping,\n        quantity,\n        images,\n        color} = values;\n    return (\n        <form onSubmit = {handleSubmit}>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"title\">\n                          Title\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"title\" \n                          name = \"title\"\n                          className=\"form-control\"\n                          value = {title}\n                          onChange = {handleChange}\n                          placeholder = \"Title\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"desc\">\n                          Description\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"desc\" \n                          name = \"description\"\n                          className=\"form-control\"\n                          value = {description}\n                          onChange = {handleChange}\n                          placeholder = \"Description\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"price\">\n                          Price\n                      </label>\n                      <input \n                          type=\"number\" \n                          id=\"price\" \n                          name = \"price\"\n                          className=\"form-control\"\n                          value = {price}\n                          onChange = {handleChange}\n                          placeholder = \"Price\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"shipping\">\n                          Shipping\n                      </label>\n                      <select \n                      name = \"shipping\" \n                      id = \"shipping\"\n                      className = \"form-control\"\n                      onChange = {handleChange}\n                      >\n                          <option value = \"\">Select one</option>\n                          <option value = \"No\">No</option>\n                          <option value = \"Yes\">Yes</option>\n                      </select>\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"qty\">\n                          Quantity\n                      </label>\n                      <input \n                          type=\"number\" \n                          id=\"qty\" \n                          name = \"quantity\"\n                          className=\"form-control\"\n                          value = {quantity}\n                          onChange = {handleChange}\n                          placeholder = \"Quantity\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"clr\">\n                          Color\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"clr\" \n                          name = \"color\"\n                          className=\"form-control\"\n                          value = {color}\n                          onChange = {handleChange}\n                          placeholder = \"Color of the product\"\n                          />\n                  </div>\n\n                  <div className=\"form-group\">\n                    <label htmlFor=\"Category-list\">Category</label>\n                    <select id = \"Category-list\" className = \"form-control\" onChange = {getSubs}>\n                        <option value = \"\">Select a Category</option>\n                        {categories.length > 0 && categories.map((c)=>{\n                            return (\n                            <option value ={c._id} key = {c._id} >{c.name}</option>\n                            )\n                        })}\n                    </select>\n                    </div>\n                {subDropDown && (\n                    <div className=\"form-group\">\n                    <label htmlFor=\"Sub-Category-list\">Sub Categories</label>\n                    <Select \n                    id = \"Sub-Category-list\" \n                    className = \"form-control\" \n                    mode = \"multiple\" \n                    value = {subCat}\n                    onChange = {(value) => setValues({ ...values, subCat: value })}>\n                        <Option value = \"\">Select a Category</Option>\n                        {subOptions.length > 0 && subOptions.map((s)=>{\n                            return (\n                            <Option value ={s._id} key = {s._id} >{s.name}</Option>\n                            )\n                        })}\n                    </Select>\n                </div>)}\n                  <button className=\"btn btn-outline-info mt-2\">Save</button>\n              </form>\n    )\n}\n\nexport default CreateProductForm\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/FileUpload.js",["268"],"import React from 'react';\nimport Resizer from 'react-image-file-resizer';\nimport axios from 'axios';\nimport {useSelector} from 'react-redux'\nimport { toast } from 'react-toastify';\nimport {Avatar, Badge} from 'antd';\nimport Item from 'antd/lib/list/Item';\nconst FileUpload = ({values,setValues,setLoading}) => {\n\n    const {user} = useSelector((state) => ({...state}));\n    const handleRemoval = (public_id) =>{\n        setLoading(true);\n        axios.post(`${process.env.REACT_APP_API}/remove-image`,{public_id},{\n            headers:{\n                authToken : user ? user.token :\"\",\n            }\n        }).then(res =>{\n            setLoading(false);\n            const {images} = values;\n            let newImages = images.filter((img)=>{\n                return img.public_id !== public_id\n            });\n            toast.success(\"image deleted\")\n            setValues({...values,images:newImages})\n        }).catch(err =>{\n            setLoading(false);\n            toast.error(\"Image deletion failed\")\n        })\n    }\n    const handleChange = (e)=>{\n        let files = e.target.files;\n        let uploadedFiles = values.images;\n        if(files.length > 0){\n            setLoading(true)\n            files.forEach(f => {\n                console.log(\"upload started\");\n                Resizer.imageFileResizer(f,360,360,'JPEG',100,0,(uri)=>{\n                    console.log(\"resize done\")\n                    axios.post(`${process.env.REACT_APP_API}/upload-images`,{\n                        img:uri\n                    },{\n                        headers:{\n                            authToken : user ? user.token :\"\",\n                        }\n                    }).then(res =>{\n                        setLoading(false);\n                        console.log(\"upload done\");\n                        uploadedFiles.push(res.data);\n                        setValues({...values,images:uploadedFiles})\n\n                    }).catch(err =>{\n                        setLoading(false);\n                        console.log(err);\n                        toast.error(\"File Upload failed\")\n                    })\n                },'base64');\n            });\n        }\n    }\n    return (\n        <>\n        <div className=\"row\">\n            {values.images &&\n          values.images.map((image) => (\n            <span className = \"avatar-item col-2\">\n                <Badge\n            count=\"X\"\n            key={image.public_id}\n            onClick = {() => handleRemoval(image.public_id)}\n            style={{ cursor: \"pointer\" }}\n          >\n            <Avatar\n              src={image.url}\n              size={100}\n              shape=\"square\"\n              className=\"ml-3\"\n            />\n          </Badge>\n            </span>\n          ))}\n        </div>\n        <div className=\"form-group\">\n            <label htmlFor=\"img\">\n                Upload Image(s)\n            </label>\n            <input \n                type=\"file\" \n                multiple\n                id=\"img\" \n                name = \"images\"\n                className=\"form-control\"\n                accept = \"images/*\"\n                onChange = {handleChange}\n            />\n        </div>\n        </>\n    )\n}\n\nexport default FileUpload\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/SearchForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/co