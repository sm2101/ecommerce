\",\n              description:\"\",\n              price:\"\",\n              category:\"\",\n              categories:[],\n              subCat:[],\n              shipping:\"\",\n              quantity:\"\",\n              images:[],\n              color:\"\"\n          }),\n          [subOptions, setSubOptions] = useState([]),\n          [subDropDown, setSubDropDown] = useState(false);\n\n    const {user} = useSelector((state) =>({...state}));\n\n    const loadCategories = () =>{\n        getAllCategories().then(res =>{\n            setValues({...values,  categories: res.data});\n        }).catch(err =>{\n            console.log(err)\n        })\n    }\n\n    const getSubs = (e) =>{\n        e.preventDefault()\n        const catId = e.target.value;\n        setValues({...values, subCat:[] ,category:catId});\n        getCategorySubs(catId).then(res =>{\n            setSubOptions(res.data)\n            setSubDropDown(true);\n        }).catch(err =>{\n            console.log(err);\n        })\n    }\n    useEffect(() =>{\n        loadCategories();\n    },[])\n\n    const handleSubmit = (e) =>{\n        e.preventDefault();\n        setLoading(true)\n        createProduct(values,user.token).then(res =>{\n            console.log(res)\n            window.alert(\"Product created\");\n            window.location.reload();\n            setLoading(false);\n        }).catch(err =>{\n            console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Create Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n             <CreateProdductForm \n             getSubs = {getSubs} \n             handleChange = {handleChange} \n             handleSubmit = {handleSubmit} \n             values = {values} \n             subOptions = {subOptions}\n             subDropDown = {subDropDown}\n             setValues = {setValues}\n             />\n          </div>\n          </div>\n        </div>\n      </>\n    )\n}\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/Products/AllProducts.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/Products/UpdateProduct.js",["191","192"],"import React, {useState, useEffect} from 'react';\nimport AdminNav from '../../Nav/AdminNav';\nimport {toast} from 'react-toastify';\nimport {useSelector} from 'react-redux';\nimport { \n    updateProduct,\n    getProduct\n} from \"../../../Functions/products\";\nimport {  \n    getAllCategories,\n    getCategorySubs \n} from \"../../../Functions/category\";\n// import {EditOutlined,DeleteOutlined} from \"@ant-design/icons\";\n// import { Link } from 'react-router-dom';\nimport UpdateProdductForm from '../../Forms/UpdateProductForm';\nimport FileUpload from '../../Forms/FileUpload';\nexport default function UpdateProduct({match,history}) {\n    const slug = match.params.slug;\n    const [loading, setLoading] = useState(\"\"),\n          [values, setValues] = useState({\n              title:\"\",\n              description:\"\",\n              price:\"\",\n              category:\"\",\n              shipping:\"\",\n              quantity:\"\",\n              images:[],\n              color:\"\"\n          }),\n          [subOptions, setSubOptions] = useState([]),\n          [categories, setCategories] = useState([]),\n          [subCatIds, setSubCatIds] = useState([]);\n\n    const {user} = useSelector((state) =>({...state}));\n\n    const loadProduct = () =>{\n        getProduct(slug).then(res =>{\n            setValues({...values, ...res.data})\n            getCategorySubs(res.data.category._id).then(s =>{\n                setSubOptions(s.data)\n            })\n            let arr =[]\n            res.data.subCat.map(s =>{\n                arr.push(s._id)\n            })\n            setSubCatIds(prev => arr);\n        }).catch(err =>{\n            console.log(err);\n        })\n    }\n\n    const loadCategories = () =>{\n        getAllCategories().then(res =>{\n            setCategories(res.data)\n        }).catch(err =>{\n            console.log(err)\n        })\n    }\n\n    const getSubs = (e) =>{\n        e.preventDefault()\n        const catId = e.target.value;\n        setValues({...values, subCat:[] ,category:catId});\n        getCategorySubs(catId).then(res =>{\n            setSubOptions(res.data)\n        }).catch(err =>{\n            console.log(err);\n        })\n        setSubCatIds([])\n        }\n    \n\n    useEffect(() =>{\n        loadProduct();\n        loadCategories();\n\n    },[])\n\n    const handleSubmit = (e) =>{\n        e.preventDefault();\n        setLoading(true)\n        values.subCat = subCatIds;\n        updateProduct(slug,values,user.token).then(res =>{\n            console.log(res)\n            toast.success(\"Product updated!\");\n            history.push(\"/admin/products\")\n            setLoading(false);\n        }).catch(err =>{\n            console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Update Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n             <UpdateProdductForm \n             getSubs = {getSubs} \n             handleChange = {handleChange} \n             handleSubmit = {handleSubmit} \n             values = {values} \n             categories = {categories}\n             subOptions = {subOptions}\n             setValues = {setValues}\n             subCatIds = {subCatIds}\n             setSubCatIds = {setSubCatIds}\n             />\n          </div>\n          </div>\n        </div>\n      </>\n    )\n}\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/SubCategory/CreateSubCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/AdminRoutes.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/UserRoute.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/category.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Nav/AdminNav.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/subCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/products.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/LoadingToRedirect.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CategoryForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CreateProductForm.js",["193","194","195"],"import React from 'react';\nimport {Select} from 'antd';\nconst {Option} = Select;\nconst CreateProductForm = (\n    {\n        values,\n        handleSubmit,\n        handleChange,\n        getSubs,\n        subOptions,\n        subDropDown,\n        setValues\n    }\n) => {\n    const {title,\n        description,\n        price,\n        category,\n        categories,\n        subCat,\n        s