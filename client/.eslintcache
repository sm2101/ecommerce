console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Create Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n             <CreateProdductForm \n             getSubs = {getSubs} \n             handleChange = {handleChange} \n             handleSubmit = {handleSubmit} \n             values = {values} \n             subOptions = {subOptions}\n             subDropDown = {subDropDown}\n             setValues = {setValues}\n             />\n          </div>\n          </div>\n        </div>\n      </>\n    )\n}\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/Products/AllProducts.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/Products/UpdateProduct.js",["228","229"],"import React, {useState, useEffect} from 'react';\nimport AdminNav from '../../Nav/AdminNav';\nimport {toast} from 'react-toastify';\nimport {useSelector} from 'react-redux';\nimport { \n    updateProduct,\n    getProduct\n} from \"../../../Functions/products\";\nimport {  \n    getAllCategories,\n    getCategorySubs \n} from \"../../../Functions/category\";\n// import {EditOutlined,DeleteOutlined} from \"@ant-design/icons\";\n// import { Link } from 'react-router-dom';\nimport UpdateProdductForm from '../../Forms/UpdateProductForm';\nimport FileUpload from '../../Forms/FileUpload';\nexport default function UpdateProduct({match,history}) {\n    const slug = match.params.slug;\n    const [loading, setLoading] = useState(\"\"),\n          [values, setValues] = useState({\n              title:\"\",\n              description:\"\",\n              price:\"\",\n              category:\"\",\n              shipping:\"\",\n              quantity:\"\",\n              images:[],\n              color:\"\"\n          }),\n          [subOptions, setSubOptions] = useState([]),\n          [categories, setCategories] = useState([]),\n          [subCatIds, setSubCatIds] = useState([]);\n\n    const {user} = useSelector((state) =>({...state}));\n\n    const loadProduct = () =>{\n        getProduct(slug).then(res =>{\n            setValues({...values, ...res.data})\n            getCategorySubs(res.data.category._id).then(s =>{\n                setSubOptions(s.data)\n            })\n            let arr =[]\n            res.data.subCat.map(s =>{\n                arr.push(s._id)\n            })\n            setSubCatIds(prev => arr);\n        }).catch(err =>{\n            console.log(err);\n        })\n    }\n\n    const loadCategories = () =>{\n        getAllCategories().then(res =>{\n            setCategories(res.data)\n        }).catch(err =>{\n            console.log(err)\n        })\n    }\n\n    const getSubs = (e) =>{\n        e.preventDefault()\n        const catId = e.target.value;\n        setValues({...values, subCat:[] ,category:catId});\n        getCategorySubs(catId).then(res =>{\n            setSubOptions(res.data)\n        }).catch(err =>{\n            console.log(err);\n        })\n        setSubCatIds([])\n        }\n    \n\n    useEffect(() =>{\n        loadProduct();\n        loadCategories();\n\n    },[])\n\n    const handleSubmit = (e) =>{\n        e.preventDefault();\n        setLoading(true)\n        values.subCat = subCatIds;\n        updateProduct(slug,values,user.token).then(res =>{\n            console.log(res)\n            toast.success(\"Product updated!\");\n            history.push(\"/admin/products\")\n            setLoading(false);\n        }).catch(err =>{\n            console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Update Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n             <UpdateProdductForm \n             getSubs = {getSubs} \n             handleChange = {handleChange} \n             handleSubmit = {handleSubmit} \n             values = {values} \n             categories = {categories}\n             subOptions = {subOptions}\n             setValues = {setValues}\n             subCatIds = {subCatIds}\n             setSubCatIds = {setSubCatIds}\n             />\n          </div>\n          </div>\n        </div>\n      </>\n    )\n}\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/admin/SubCategory/CreateSubCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/AdminRoutes.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/UserRoute.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/category.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Nav/AdminNav.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/subCat.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/Functions/products.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/private_routes/LoadingToRedirect.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CategoryForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/CreateProductForm.js",["230","231","232"],"import React from 'react';\nimport {Select} from 'antd';\nconst {Option} = Select;\nconst CreateProductForm = (\n    {\n        values,\n        handleSubmit,\n        handleChange,\n        getSubs,\n        subOptions,\n        subDropDown,\n        setValues\n    }\n) => {\n    const {title,\n        description,\n        price,\n        category,\n        categories,\n        subCat,\n        shipping,\n        quantity,\n        images,\n        color} = values;\n    return (\n        <form onSubmit = {handleSubmit}>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"title\">\n                          Title\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"title\" \n                          name = \"title\"\n                          className=\"form-control\"\n                          value = {title}\n                          onChange = {handleChange}\n                          placeholder = \"Title\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"desc\">\n                          Description\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"desc\" \n                          name = \"description\"\n                          className=\"form-control\"\n                          value = {description}\n                          onChange = {handleChange}\n                          placeholder = \"Description\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"price\">\n                          Price\n                      </label>\n                      <input \n                          type=\"number\" \n                          id=\"price\" \n                          name = \"price\"\n                          className=\"form-control\"\n                          value = {price}\n                          onChange = {handleChange}\n                          placeholder = \"Price\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"shipping\">\n                          Shipping\n                      </label>\n                      <select \n                      name = \"shipping\" \n                      id = \"shipping\"\n                      className = \"form-control\"\n                      onChange = {handleChange}\n                      >\n                          <option value = \"\">Select one</option>\n                          <option value = \"No\">No</option>\n                          <option value = \"Yes\">Yes</option>\n                      </select>\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"qty\">\n                          Quantity\n                      </label>\n                      <input \n                          type=\"number\" \n                          id=\"qty\" \n                          name = \"quantity\"\n                          className=\"form-control\"\n                          value = {quantity}\n                          onChange = {handleChange}\n                          placeholder = \"Quantity\"\n                          />\n                  </div>\n                  <div className=\"form-group\">\n                      <label htmlFor=\"clr\">\n                          Color\n                      </label>\n                      <input \n                          type=\"text\" \n                          id=\"clr\" \n                          name = \"color\"\n                          className=\"form-control\"\n                          value = {color}\n                          onChange = {handleChange}\n                          placeholder = \"Color of the product\"\n                          />\n                  </div>\n\n                  <div className=\"form-group\">\n                    <label htmlFor=\"Category-list\">Category</label>\n                    <select id = \"Category-list\" className = \"form-control\" onChange = {getSubs}>\n                        <option value = \"\">Select a Category</option>\n                        {categories.length > 0 && categories.map((c)=>{\n                            return (\n                            <option value ={c._id} key = {c._id} >{c.name}</option>\n                            )\n                        })}\n                    </select>\n                    </div>\n                {subDropDown && (\n                    <div className=\"form-group\">\n                    <label htmlFor=\"Sub-Category-list\">Sub Categories</label>\n                    <Select \n                    id = \"Sub-Category-list\" \n                    className = \"form-control\" \n                    mode = \"multiple\" \n                    value = {subCat}\n                    onChange = {(value) => setValues({ ...values, subCat: value })}>\n                        <Option value = \"\">Select a Category</Option>\n                        {subOptions.length > 0 && subOptions.map((s)=>{\n                            return (\n                            <Option value ={s._id} key = {s._id} >{s.name}</Option>\n                            )\n                        })}\n                    </Select>\n                </div>)}\n                  <button className=\"btn btn-outline-info mt-2\">Save</button>\n              </form>\n    )\n}\n\nexport default CreateProductForm\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/FileUpload.js",["233"],"import React from 'react';\nimport Resizer from 'react-image-file-resizer';\nimport axios from 'axios';\nimport {useSelector} from 'react-redux'\nimport { toast } from 'react-toastify';\nimport {Avatar, Badge} from 'antd';\nimport Item from 'antd/lib/list/Item';\nconst FileUpload = ({values,setValues,setLoading}) => {\n\n    const {user} = useSelector((state) => ({...state}));\n    const handleRemoval = (public_id) =>{\n        setLoading(true);\n        axios.post(`${process.env.REACT_APP_API}/remove-image`,{public_id},{\n            headers:{\n                authToken : user ? user.token :\"\",\n            }\n        }).then(res =>{\n            setLoading(false);\n            const {images} = values;\n            let newImages = images.filter((img)=>{\n                return img.public_id !== public_id\n            });\n            toast.success(\"image deleted\")\n            setValues({...values,images:newImages})\n        }).catch(err =>{\n            setLoading(false);\n            toast.error(\"Image deletion failed\")\n        })\n    }\n    const handleChange = (e)=>{\n        let files = e.target.files;\n        let uploadedFiles = values.images;\n        if(files.length > 0){\n            setLoading(true)\n            files.forEach(f => {\n                console.log(\"upload started\");\n                Resizer.imageFileResizer(f,360,360,'JPEG',100,0,(uri)=>{\n                    console.log(\"resize done\")\n                    axios.post(`${process.env.REACT_APP_API}/upload-images`,{\n                        img:uri\n                    },{\n                        headers:{\n                            authToken : user ? user.token :\"\",\n                        }\n                    }).then(res =>{\n                        setLoading(false);\n                        console.log(\"upload done\");\n                        uploadedFiles.push(res.data);\n                        setValues({...values,images:uploadedFiles})\n\n                    }).catch(err =>{\n                        setLoading(false);\n                        console.log(err);\n                        toast.error(\"File Upload failed\")\n                    })\n                },'base64');\n            });\n        }\n    }\n    return (\n        <>\n        <div className=\"row\">\n            {values.images &&\n          values.images.map((image) => (\n            <span className = \"avatar-item col-2\">\n                <Badge\n            count=\"X\"\n            key={image.public_id}\n            onClick = {() => handleRemoval(image.public_id)}\n            style={{ cursor: \"pointer\" }}\n          >\n            <Avatar\n              src={image.url}\n              size={100}\n              shape=\"square\"\n              className=\"ml-3\"\n            />\n          </Badge>\n            </span>\n          ))}\n        </div>\n        <div className=\"form-group\">\n            <label htmlFor=\"img\">\n                Upload Image(s)\n            </label>\n            <input \n                type=\"file\" \n                multiple\n                id=\"img\" \n                name = \"images\"\n                className=\"form-control\"\n                accept = \"images/*\"\n                onChange = {handleChange}\n            />\n        </div>\n        </>\n    )\n}\n\nexport default FileUpload\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/SearchForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Forms/UpdateProductForm.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/cards/ProductCard.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/cards/UserProductCard.js",["234"],"import React from 'react'\nimport { Card } from \"antd\";\nimport {useHistory} from \"react-router-dom\";\nimport {Link} from \"react-router-dom\"\nimport {EyeOutlined, ShoppingCartOutlined} from \"@ant-design/icons\"\nimport {ShowAverage} from '../../Functions/rating'\nconst { Meta } = Card;\nconst UserProductCard = ({product}) => {\n    const { title, description, images, slug } = product;\n\n    return (\n      <>\n      <div id = \"rating\">\n      {product && product.rating && product.rating.length >0 ? \n            ShowAverage(product) : (\n            <div className = \"text-center py-2\">\n                No Ratings yet!\n            </div>)}\n      </div>\n        <Card className = \"p-2\"\n      cover={\n        <img\n          src={images && images.length ? images[0].url : \"https://i1.wp.com/fremontgurdwara.org/wp-content/uploads/2020/06/no-image-icon-2.png\"}\n          style={{ height: \"150px\", objectFit: \"cover\" }}\n          alt=\"Product\"\n        />\n      }\n      actions = {[\n        <Link to = {`/product/${slug}`}>\n          <EyeOutlined \n          className = \"text-primary\"\n          /> <br />\n          <span className = \"text-primary card-action-text\">View Product</span>\n        </Link>,\n        <>\n        <ShoppingCartOutlined \n          className = \"text-primary\"\n          /><br />\n          <span className = \"text-primary card-action-text\">Add to cart</span>\n        </>\n      ]}\n    >\n      <Meta \n      title={title} \n      description={`${description && description.substring(0,50)}...`} \n      />\n    </Card>\n    </>\n    )\n}\n\nexport default UserProductCard\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/cards/SkeletonCard.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Landing/NewArrivals.js",["235"],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Landing/BestSeller.js",["236"],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Product.js",["237","238","239"],"import React ,{useState, useEffect} from 'react'\nimport {getProduct, productStar, getRelated} from '../Functions/products'\nimport ProductPage from '../components/cards/ProductPage';\nimport {useSelector} from 'react-redux'\nimport {toast} from 'react-toastify'\nimport UserProductCard from '../components/cards/UserProductCard'\nimport SkeletonCard from '../components/cards/SkeletonCard'\n\nconst Product = ({match}) => {\n    const [product, setProduct] = useState({}),\n          [star, setStar] = useState(0),\n          [loading,setLoading] = useState(false),\n          [related, setRelated] = useState([]);\n\n    const {user} = useSelector((state) => ({...state}));\n    \n    const {slug} = match.params;\n\n    const onStarClick = (newRating, name) =>{\n        setStar(newRating);\n        productStar(name,newRating,user.token).then(res=>{\n            console.log(res);\n            toast.success(\"Thanks for rating the product!\")\n        }).catch(err =>{\n\n        })\n    }\n    useEffect(()=>{\n        loadProduct();\n    },[slug])\n    useEffect(()=>{\n        if(product.rating&&user){\n            let exist = product.rating.find((ele) => (\n                ele.postedBy == user._id\n            ));\n            exist&& setStar(exist.star);\n        }\n    })\n    const loadProduct = () => {\n        setLoading(true)\n        getProduct(slug).then(res =>{\n            setProduct(res.data)\n            getRelated(res.data._id).then(r =>{\n                setLoading(false)\n                setRelated(r.data)\n            })\n        })\n    }\n\n    return (\n        <div className = \"container-fluid m-0\">\n            <div className=\"container\">\n            <div className=\"row\">\n                <ProductPage product = {product} star = {star} onStarClick = {onStarClick} />\n            </div>\n            <div className=\"row\">\n                <div className=\"col-12 text-center\">\n                    <hr />\n                    <h4>Related Products</h4>\n                </div>\n            {loading?(\n                    <SkeletonCard count = {4}/>\n                ):(<div className = \"row\">\n                    {related.length > 0 ?related.map(p =>(\n                        <div className = \"col-12 col-md-6 col-lg-3\" key = {p._id}>\n                            <UserProductCard product = {p}/>\n                        </div>\n                    )) : <div className=\"col-12 text-center\">\n                        No related product found\n                        </div>}\n                </div>)}\n            </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Product\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/cards/ProductPage.js",["240","241","242"],"import React from 'react';\nimport {Card, Tabs } from 'antd';\nimport {Link} from 'react-router-dom';\nimport {HeartOutlined, ShoppingCartOutlined, StarOutlined} from '@ant-design/icons';\nimport \"react-responsive-carousel/lib/styles/carousel.min.css\";\nimport {Carousel} from 'react-responsive-carousel';\nimport StartRatings from 'react-star-ratings';\nimport RatingModal from '../modal/RatingModal';\nimport {ShowAverage} from '../../Functions/rating'\nconst {TabPane} = Tabs;\nconst ProductPage = ({product, star, onStarClick}) => {\n    const {\n        title,\n        description,\n        images,\n        slug,\n        price,\n        color,\n        category,\n        subCat,\n        shipping,\n        quantity,\n        _id\n    } = product\n    return (\n        <>\n        <div className=\"col-12 col-md-7\">\n            <div className=\"row\">\n            {images && images.length ?<Carousel \n            showArrows = {true} \n            autoPlay \n            infiniteLoop\n            className = \"height-25\"\n            >\n                {images && images.map(i => (\n                    <img src = {i.url} key={i.public_id}></img>\n                ))}\n            </Carousel> :\n            <Card\n            cover = {\n                <img\n                    src=\"https://i1.wp.com/fremontgurdwara.org/wp-content/uploads/2020/06/no-image-icon-2.png\"\n                    style={{ height: \"25rem\", objectFit: \"cover\" }}\n                    alt=\"Default\"\n                />\n            }\n            ></Card> \n            }\n            </div>\n            <div className=\"row mt-3\">\n                <Tabs type = \"card\">\n                    <TabPane tab = \"Description\" key = \"1\">\n                        {description}\n                    </TabPane>\n                </Tabs>\n            </div>\n        </div>\n        <div className=\"col-12 col-md-5\">\n            <div>\n            <h1 className = \"display-3\">{title}</h1>\n            {product && product.rating && product.rating.length >0 ? \n            ShowAverage(product) : (\n            <div className = \"text-center py-2\">\n                No Ratings yet!\n            </div>)}\n            </div>\n            <Card\n            actions = {[\n                <>\n                <ShoppingCartOutlined className = \"text-success\"/>Add to Cart\n                </>,\n                <>\n                <HeartOutlined className = \"text-danger\"/> Add to wishLisht\n                </>,\n                <RatingModal>\n                <StartRatings\n                    name = {_id}\n                    numberOfStars ={5}\n                    rating = {star}\n                    changeRating = {onStarClick}\n                    isSelectable = {true}\n                    starRatedColor = \"#437846\" \n                    />\n                </RatingModal>\n            ]}\n            >\n                <ul className = \"list-group-flush p-0\">\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Price\n                        </span>\n                        <span>\n                            {price}\n                        </span>\n                    </li>\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Category\n                        </span>\n                        <span>\n                            {category && (<Link to = {`/category/${category.slug}`}>\n                            {category.name}\n                            </Link>)}\n                        </span>\n                    </li>\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Sub Categories\n                        </span>\n                        {subCat && subCat.map(s =>(\n                            <span>\n                                <Link to = {`/sub-category/${s.slug}`}>\n                                    {s.name}\n                                </Link>\n                            </span>\n                        ))}\n                    </li>\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Shipping\n                        </span>\n                        <span>\n                            {shipping}\n                        </span>\n                    </li>\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Color\n                        </span>\n                        <span>\n                            {color}\n                        </span>\n                    </li>\n                    <li className=\"list-group-item d-flex justify-content-between\">\n                        <span>\n                            Quantity\n                        </span>\n                        <span>\n                            {quantity}\n                        </span>\n                    </li>\n                </ul>\n            </Card>\n        </div>\n        </>\n    )\n}\n\nexport default ProductPage\n","/home/sm2101/Documents/projects/ecommerce/client/src/components/modal/RatingModal.js",["243"],"import React , {useState} from 'react'\nimport {Modal, Button} from 'antd'\nimport {toast} from 'react-toastify'\nimport {useSelector} from 'react-redux'\nimport {StarOutlined} from '@ant-design/icons';\nimport {useHistory, useParams} from 'react-router-dom';\nconst RatingModal = ({children }) => {\n    const {user} = useSelector((state) => ({...state}));\n    const [visible, setVisibile] = useState(false);\n\n    const history = useHistory();\n    const {slug} = useParams();\n    const handleModal = () =>{\n        if(user && user.token){\n            setVisibile(true);\n        } else {\n            history.push({\n                pathname:'/login',\n                state:{\n                    from:`product/${slug}`\n                }\n            });\n        }\n    }\n\n    return (\n        <>\n          <div\n          onClick = {handleModal}\n          >\n              <StarOutlined className = \"text-warning\"/><br/>\n              {user?\"Leave a rating\" : \"Login to leave a rating\"}\n            </div>  \n            <Modal\n            title = \"Leave your rating\"\n             centered\n             visible = {visible}\n             onOk = {() =>{\n                 setVisibile(false)\n                 toast.success(\"Rating Saved!\")\n                }}\n                onCancel = {()=>{\n                    setVisibile(false)\n                }}\n            >\n                {children}\n            </Modal>\n        </>\n    )\n}\n\nexport default RatingModal\n","/home/sm2101/Documents/projects/ecommerce/client/src/Functions/rating.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Landing/Categories.js",[],"/home/sm2101/Documents/projects/ecommerce/client/src/components/Category/CategoryHome.js",["244","245","246"],"import React, {useState, useEffect} from 'react'\nimport {getCategory} from '../../Functions/category';\nimport {Link} from 'react-router-dom'\nimport UserProductCard from '../cards/UserPro