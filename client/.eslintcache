ubOptions(s.data)\n            })\n            let arr =[]\n            res.data.subCat.map(s =>{\n                arr.push(s._id)\n            })\n            setSubCatIds(prev => arr);\n        }).catch(err =>{\n            console.log(err);\n        })\n    }\n\n    const loadCategories = () =>{\n        getAllCategories().then(res =>{\n            setCategories(res.data)\n        }).catch(err =>{\n            console.log(err)\n        })\n    }\n\n    const getSubs = (e) =>{\n        e.preventDefault()\n        const catId = e.target.value;\n        setValues({...values, subCat:[] ,category:catId});\n        getCategorySubs(catId).then(res =>{\n            setSubOptions(res.data)\n        }).catch(err =>{\n            console.log(err);\n        })\n        setSubCatIds([])\n        }\n    \n\n    useEffect(() =>{\n        loadProduct();\n        loadCategories();\n\n    },[])\n\n    const handleSubmit = (e) =>{\n        e.preventDefault();\n        setLoading(true)\n        values.subCat = subCatIds;\n        updateProduct(slug,values,user.token).then(res =>{\n            console.log(res)\n            toast.success(\"Product updated!\");\n            history.push(\"/admin/products\")\n            setLoading(false);\n        }).catch(err =>{\n            console.log(err);\n            toast.error(\"Some error occured\")\n            setLoading(false);\n        })\n    }\n    const handleChange = (e) =>{\n        e.preventDefault();\n        setValues({...values,[e.target.name]:e.target.value})\n    }\n    return (\n        <>\n        <div className=\"container-fluid\">\n          <div className=\"row\">\n            <div className=\"col-md-2\">\n              <AdminNav />\n            </div>\n            <div className=\"col\">\n              {loading?<h3>Loading...</h3>:<h3>Update Product</h3>}\n              <FileUpload \n              values = {values}\n              setValues = {setValues}\n              setLoading = {setLoading}\n              />\n         